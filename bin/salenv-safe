if /bin/sh -ec "ulimit" > /dev/null 2> /dev/null; then
    ulimit -s unlimited > /dev/null 2> /dev/null
    if [ $? -ne 0 ]; then
	ulimit -Ss unlimited > /dev/null 2> /dev/null
    fi
fi
SALENV_DIR=/Users/e35480/projects/misc/sal/sal-3.3;
export SALENV_DIR;
PATH=$PATH:$SALENV_DIR/bin; 
export PATH;
BIGLOOLIB=/Users/e35480/projects/misc/sal/sal-3.3/lib/arm-apple-darwin24.6.0-release:/opt/homebrew/Cellar/bigloo/4.6a/lib/bigloo/4.6a
export BIGLOOLIB;
if test -z "$ICS_LICENSE_CERTIFICATE"; then ICS_LICENSE_CERTIFICATE=$SALENV_DIR/certificate; export ICS_LICENSE_CERTIFICATE; fi;
cmd="exec /Users/e35480/projects/misc/sal/sal-3.3/bin/salenv-exec-safe $*"; 
eval $cmd

