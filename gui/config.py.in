sal_front_end_code = "@sal_front_end_code@"
sal_scripts = "@sal_scripts@"
salenv_exec = "@salenv_exec@"
ARCH = "@ARCH@"
SALENV_BUILD_MODE= "@SALENV_BUILD_MODE@"
EXEEXT="@EXEEXT@"
ICS_BIN_DIR="@ICS_BIN_DIR@"

bindir="../bin/%s-%s" % (ARCH, SALENV_BUILD_MODE)

def GetFrontEndCode(): 
		return sal_front_end_code.split(' ')
def GetScripts(): 
		return sal_scripts.split(' ')
def GetBinDir():
    return bindir
def GetICSBinDir():
    return ICS_BIN_DIR
def GetSALExec():
    return salenv_exec
def GetExeExt():
    return EXEEXT
